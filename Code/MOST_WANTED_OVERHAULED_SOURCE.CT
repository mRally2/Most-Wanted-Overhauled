[ENABLE]
math.randomseed(os.time())
addresslist3 = getAddressList()
CheatEntry3 = addresslist_getMemoryRecordByDescription(addresslist3,[[GAS STATION]])
local isTODChanged = false
local isWeatherChanged = false
function randomFloat(lower, greater)
    return lower + math.random()  * (greater - lower);
end
function welcomeFH()
    if readPointer("[currentevent]") ~= nil then
        if readInteger("[currentevent]") ~= 1560354 then --WELCOME TO FAIRHAVEN
            memoryrecord_freeze(CheatEntry3) --GAS STATION ENABLED
        else
            memoryrecord_unfreeze(CheatEntry3) --GAS STATION DISABLED
            if isWeatherChanged == false then
                --CUT RAINY WEATHER
                if math.random(1,4) == 1 then
                    --ALTERNATIVE
                    autoAssemble([[
                    "NFS13.exe"+27F5F9:
                    cmp byte ptr [eax+edi+46],01
                    ]])
                    --ROAD WETNESS
                    autoAssemble([[
                    "NFS13.exe"+27F5C2:
                    cmp word ptr [edx+edi+40],01
                    ]])
                    --THUNDER
                    autoAssemble([[
                    0067F60A:
                    mov byte ptr [edx+edi+44],01
                    ]])
                else
                    --ALTERNATIVE
                    if math.random(0,1) == 0 then
                      autoAssemble([[
                      "NFS13.exe"+27F5F9:
                      cmp byte ptr [eax+edi+46],01
                      ]])
                    else
                      autoAssemble([[
                      "NFS13.exe"+27F5F9:
                      cmp byte ptr [eax+edi+46],00
                      ]])
                    end
                    --ROAD WETNESS
                    if math.random(0,1) == 0 then
                      autoAssemble([[
                      "NFS13.exe"+27F5C2:
                      cmp word ptr [edx+edi+40],01
                      ]])
                    else
                      autoAssemble([[
                      "NFS13.exe"+27F5C2:
                      cmp word ptr [edx+edi+40],00
                      ]])
                    end
                    --THUNDER
                    autoAssemble([[
                    0067F60A:
                    cmp byte ptr [edx+edi+44],00
                    ]])
                end
                isWeatherChanged = true
           end
           if readFloat("[pTimeOfDay]+2A84") &gt; 64799.0 and isTODChanged == false then
               --TIME OF DAY
               writeFloat("[pTimeOfDay]+2A84",randomFloat(0,86400))
               isTODChanged = true
           end
        end
    end
end

wFH=createTimer(nil)
timer_setInterval(wFH, 1000)
timer_onTimer(wFH, welcomeFH)
timer_setEnabled(wFH, true)

[DISABLE]
