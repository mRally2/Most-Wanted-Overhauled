        elseif readFloat("[heat]+B0") &lt; 0.15 and readFloat("[heat]+B0") &gt; 0.01 then
           deltaLo = 3.5
           deltaHi = 10.0
           isCritical = true
        elseif readFloat("[heat]+B0") &lt; 0.01 then
           deltaLo = 5.0
           deltaHi = 10.0
           isCritical = true
        else
           isCritical = false
           writeFloat("[postfx]+30",baseValue)
        end
        if isCritical == true then
            if ascending == true then
               writeFloat("[postfx]+30",readFloat("[postfx]+30")+deltaLo*1.7183)
            end
            if readFloat("[postfx]+30") &gt; deltaHi then
               ascending = false
            end
            if ascending == false then
               if readFloat("[postfx]+30")-deltaLo*1.7183 &gt; 0 then
                  writeFloat("[postfx]+30",readFloat("[postfx]+30")-deltaLo*1.7183)
               else
                  writeFloat("[postfx]+30",0.0)
                  ascending = true
               end
            end
        end
    end
end

