function fade()
    if readPointer("[postfx]") ~= nil and readPointer("[heat]") ~= nil then
        if readFloat("[heat]+B0") &lt; 0.5 and readFloat("[heat]+B0") &gt; 0.35 then
           deltaLo = 0.5
