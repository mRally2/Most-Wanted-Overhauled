[ENABLE]
local updated = false
local prevbounty = -1
local bustbounty = 0
function bountyCheck()
  if readPointer("[globalbounty]") ~= nil and readPointer("[heat]") ~= nil then
     --CHECK FOR NEGATIVE GLOBAL BOUNTY BALANCE
     if readInteger("[globalbounty]") &gt; 50000000 then
        writeInteger("[globalbounty]",0)
     end

     --WRITE GLOBAL INTO HUD
     writeInteger("[heat]-A43C",readInteger("[globalbounty]"))

     --PREPARE NEGATIVE NUMBER IF BUSTED
     if readInteger("[heat]-A438") &gt; 0 then
        bustbounty = readInteger("[heat]-A438")
        bustbounty = -bustbounty
     end

     --NEW COP CHASE IS ENGAGED
     if prevbounty ~= readInteger("[globalbounty]") and readFloat("[heat]") == 0 then
        updated = false
     end

     --PLAYER IS BUSTED
     if readByte("[heat]+1E4C9") == 1 and readInteger("[heat]+1E33C") == 1065353216 and updated == false then
        prevbounty = readInteger("[globalbounty]")
        writeInteger("[globalbounty]",readInteger("[globalbounty]")+bustbounty)
        updated = true
     end
  end
end

bC=createTimer(nil)
timer_setInterval(bC, 100)
timer_onTimer(bC, bountyCheck)
timer_setEnabled(bC, true)

